<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.UploadAlbum">
   <children>
      <TextField fx:id="albumTitle" promptText="Album Title" />
      <Button fx:id="uploadSongs" alignment="CENTER" mnemonicParsing="false" onAction="#handleUploadSongs" text="Upload Songs" textAlignment="CENTER" />
      <Button fx:id="uploadCoverArt" alignment="CENTER" mnemonicParsing="false" onAction="#handleUploadCoverArt" text="Upload Cover Art" textAlignment="CENTER" />
      <Button fx:id="submit" alignment="CENTER" mnemonicParsing="false" onAction="#handleSubmit" text="Submit" textAlignment="CENTER" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
